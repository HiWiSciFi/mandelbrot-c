# amount of algorithm iterations
iterations:50

# size of the exported image in pixels
imageWidth:2000
imageHeight:2000

# center coordinates of the rendered image
locationX:0
locationY:0

# zoom factor
zoom:1

# color mode (0 | 1 | 2)
# 0 -> gradient between 3 colors
# 1 -> infinite gradient
# 2 -> predefined array gradient
colorMode:0

# amount of frames to render
frameCount:1

# zoom speed (ignored if frameCount is 1)
zoomSpeed:1.1
